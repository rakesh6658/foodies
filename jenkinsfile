pipeline{
    agent any
    environment{
        name="rakesh"
        version="1.0"
    }
    stages{
        stage('checkout scm'){
            steps{
                echo "checkout code from scm"
                echo " name ${name}"
                echo "version ${version}"
            }
        }
        stage('buuild'){
            steps{
                echo "building the project"
            }
        }
    }
}